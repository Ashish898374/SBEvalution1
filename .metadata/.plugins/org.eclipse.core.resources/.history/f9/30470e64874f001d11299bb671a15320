package check;

public class pra  implements Runnable{
 int sum;
 
	@Override
	public void run() {
		// TODO Auto-generated method stub
		for(int i=1;i<10;i++) {
			sum+=i;
			try {
				Thread.sleep(2000);
			} catch (InterruptedException e) {
			  e.printStackTrace();
			}
			System.out.println("start"+i);
		}
		
	}

}
